<h1>Listing test2s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Temperature</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test2s.each do |test2| %>
      <tr>
        <td><%= test2.name %></td>
        <td><%= test2.temperature %></td>
        <td><%= link_to 'Show', test2 %></td>
        <td><%= link_to 'Edit', edit_test2_path(test2) %></td>
        <td><%= link_to 'Destroy', test2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test2', new_test2_path %>
